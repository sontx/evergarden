{
  "name": "@evergarden/api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run compile",
    "compile": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "cross-env NODE_ENV=development nest start",
    "start:dev": "cross-env NODE_ENV=development concurrently \"npm:start:watch\" \"npm:start:storage\"",
    "start:watch": "cross-env NODE_ENV=development nest start --watch",
    "docker:dev": "npm run compile && cross-env NODE_ENV=development nest start --watch --tsc",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:storage": "serve upload/storage -l 2000",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n DbChange",
    "typeorm:run": "npm run typeorm migration:run",
    "typeorm:revert": "npm run typeorm migration:revert",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.migration.ts"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.13.0",
    "@evergarden/shared": "1.0.0",
    "@nestjs/common": "^7.6.17",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.17",
    "@nestjs/elasticsearch": "^7.1.0",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.17",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/swagger": "^4.8.0",
    "@nestjs/typeorm": "^7.1.5",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^9.0.2",
    "google-auth-library": "^7.1.1",
    "image-size": "^1.0.0",
    "imagemin": "^7.0.1",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-pngquant": "^9.0.2",
    "lodash.throttle": "^4.1.1",
    "minio": "^7.0.18",
    "ms": "^2.1.3",
    "mysql2": "^2.2.5",
    "nanoid": "^3.1.23",
    "nestjs-redis": "^1.3.3",
    "nestjs-typeorm-paginate": "^2.5.1",
    "passport": "^0.4.1",
    "passport-google-oauth": "^2.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "sharp": "^0.28.3",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.36",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.17",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.12",
    "@types/imagemin": "^7.0.0",
    "@types/imagemin-jpegtran": "^5.0.0",
    "@types/jest": "^26.0.22",
    "@types/lodash.throttle": "^4.1.6",
    "@types/minio": "^7.0.9",
    "@types/ms": "^0.7.31",
    "@types/node": "^14.17.2",
    "@types/passport-google-oauth20": "^2.0.7",
    "@types/passport-jwt": "^3.0.5",
    "@types/sharp": "^0.28.3",
    "@types/supertest": "^2.0.10",
    "@types/valid-url": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.3.0",
    "serve": "^11.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.3.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage"
  }
}
